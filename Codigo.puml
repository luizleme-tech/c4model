@startuml
!include https://raw.githubusercontent.com/plantuml-stdlib/C4-PlantUML/master/C4_Deployment.puml

class Book {
    - String title
    - String author
    - Date publishedDate
    - boolean isAvailable

    + getTitle() : String
    + getAuthor() : String
    + getPublishedDate() : Date
    + checkAvailability() : boolean
}

@enduml